{
	"$schema": "https://unpkg.com/wrangler/config-schema.json",
	"name": "platform-authentication-rpc",
	"main": "./main.ts",
	"compatibility_date": "2025-04-05",
	"compatibility_flags": ["nodejs_compat"],
	"keep_vars": false,
	"minify": true,
	"observability": {
		"enabled": true,
		"head_sampling_rate": 1,
		"logs": {
			"enabled": true,
			"invocation_logs": true,
			"head_sampling_rate": 1
		}
	},
	"placement": {
		"mode": "smart"
	},
	"workers_dev": false,
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "platform-authentication",
			"database_id": "e28d6a09-2bed-4a08-9c57-4db2e2a222e5",
			"migrations_dir": "../data-model/migrations"
		}
	],
	"secrets_store_secrets": [
		{
			"store_id": "492e5e40b9d64ebeac7e7a77db91ff6e",
			"secret_name": "GITHUB_OAUTH_CLIENT_ID",
			"binding": "GITHUB_OAUTH_CLIENT_ID",
		},
				{
			"store_id": "492e5e40b9d64ebeac7e7a77db91ff6e",
			"secret_name": "GITHUB_OAUTH_CLIENT_SECRET",
			"binding": "GITHUB_OAUTH_CLIENT_SECRET",
		},
	],
}
