{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"styled-system/*": ["styled-system/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	},
	"include": [".astro/types.d.ts", "**/*", "styled-system"],
	"exclude": ["dist", "node_modules"]
}
