{
	"$schema": "./node_modules/wrangler/config-schema.json",
	"name": "klustered-dev-website",
	"main": "@astrojs/cloudflare/entrypoints/server",
	"compatibility_date": "2025-05-05",
	"compatibility_flags": ["nodejs_compat"],
	"observability": {
		"traces": {
			"enabled": true,
			"destinations": ["grafana-traces"]
		},
		"logs": {
			"enabled": true,
			"destinations": ["grafana-logs"]
		}
	},
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist",
		"run_worker_first": true
	},
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "klustered-brackets",
			"database_id": "REPLACE_WITH_DATABASE_ID",
			"migrations_dir": "./src/db/migrations"
		}
	],
	"kv_namespaces": [
		{
			"binding": "SESSION",
			"id": "6c667fcff195414c8f15bf85aa0bc21d"
		}
	],
	"routes": [
		{
			"pattern": "klustered.dev",
			"custom_domain": true
		}
	],
	"services": [
		{
			"binding": "IDENTITY",
			"service": "rawkode-academy-identity"
		},
		{
			"binding": "EMAIL_PREFERENCES",
			"service": "platform-email-preferences-rpc",
			"entrypoint": "EmailPreferences"
		}
	]
}
